<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Native Stats</title>
    <link rel="stylesheet" href="~/lib/swipper/css/swiper-bundle.min.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <script src="~/lib/swipper/js/swiper-bundle.min.js" defer></script>
    
    <script src="~/js/custom-container.js" defer></script>
    <script src="~/js/custom-loader.js" defer></script>
    <script src="~/js/error-message.js" defer></script>
</head>
<body>
    <main>
        @RenderBody()
    </main>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
